<AnchorPane prefHeight="525.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.GUI.startWindowController">
   <children>
      <HBox prefHeight="25.0" prefWidth="700.0">
