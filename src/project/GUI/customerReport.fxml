   <children>
      <Button fx:id="chooseThisCustomerBtn" layoutX="16.0" layoutY="345.0" mnemonicParsing="false" onAction="#chooseCustomer" prefHeight="27.0" prefWidth="156.0" text="Choose this customer" />
