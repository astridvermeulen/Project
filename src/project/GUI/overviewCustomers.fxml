<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.GUI.OverviewCustomersController">
   <children>
      <TableView fx:id="tableViewCustomers" prefHeight="350.0" prefWidth="676.0">
        <columns>
          <TableColumn fx:id="firstNameColumn" prefWidth="173.0" text="First name" />
          <TableColumn fx:id="lastNameColumn" prefWidth="270.0" text="Last name" />
            <TableColumn fx:id="passportNumberColumn" prefWidth="118.0" text="Passport number" />
            <TableColumn fx:id="homeCountryColumn" prefWidth="114.0" text="Home country" />
        </columns>
      </TableView>
      <TextField fx:id="firstNameTxtField" layoutX="21.0" layoutY="358.0" prefHeight="27.0" prefWidth="130.0" promptText="First name" />
      <TextField fx:id="lastNameTxtField" layoutX="167.0" layoutY="358.0" prefHeight="27.0" prefWidth="120.0" promptText="Last name" />
      <TextField fx:id="passportIDTxtField" layoutX="301.0" layoutY="358.0" prefHeight="27.0" prefWidth="134.0" promptText="Passport number" />
      <Button fx:id="addBtn" layoutX="456.0" layoutY="358.0" mnemonicParsing="false" onAction="#addBtnClicked" text="Add or modify" />
      <Button fx:id="deleteBtn" layoutX="575.0" layoutY="358.0" mnemonicParsing="false" onAction="#deleteBtnClicked" text="Delete" />
   </children>
</AnchorPane>
