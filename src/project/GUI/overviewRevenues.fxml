<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.GUI.OverviewRevenuesController">
   <children>
      <Label fx:id="chooseYearLbl" layoutX="14.0" layoutY="72.0" prefHeight="17.0" prefWidth="207.0" text="Choose for which year you want " />
      <ChoiceBox fx:id="yearChoiceBox" layoutX="13.0" layoutY="125.0" prefWidth="150.0" />
      <Label layoutX="14.0" layoutY="89.0" text="to request the revenues" />
      <Button fx:id="showBtn" layoutX="13.0" layoutY="164.0" mnemonicParsing="false" onAction="#showRevenues" text="Show revenues" />
      <TableView fx:id="revenueTableView" layoutX="225.0" layoutY="72.0" prefHeight="297.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="monthColumn" prefWidth="75.0" text="Month" />
          <TableColumn fx:id="revenueColumn" prefWidth="124.0" text="Revenue" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
